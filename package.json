{
  "name": "qbraid-chat",
  "displayName": "qBraid Chat",
  "description": "VS Code extension to interact with qBraid AI chat models.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qbraid-chat.setApiKey",
        "title": "Set qBraid API Key"
      },
      {
        "command": "qbraid-chat.openChat",
        "title": "Open qBraid Chat"
      }
    ]
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch",
    "package": "npm run compile && webpack --mode production --devtool hidden-source-map",
    "vsce:package": "npm run package && vsce package"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "qbraid-chat": "file:"
  }
}